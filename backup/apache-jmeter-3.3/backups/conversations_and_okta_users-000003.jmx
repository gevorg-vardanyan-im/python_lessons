                <stringProp name="script">import java.util.LinkedHashMap;


private String createFileHeader(LinkedHashMap usersInfo) {
	StringBuilder fileHeader = new StringBuilder(&quot;&quot;);
	for (String key : usersInfo.keySet()) {
		String keyString = (String)key;
		fileHeader.append(keyString + &quot;, &quot;);
	}
	//rm last redundant &apos;, &apos; symbols
	String header = fileHeader.toString();
	header = header.substring(0, header.length() - 2);

	return header;
}

private boolean doesFileContainLine(String file, String string_line) {
	boolean does_file_contain_line = false;
	reader = new BufferedReader(new FileReader(file));
	String line = reader.readLine();
	while (line != null) {
		if (line.contains(string_line)){
			does_file_contain_line = true;
			break;
		}
		line = reader.readLine();
	}
	reader.close();

	return does_file_contain_line;
}

private String createUsersData(LinkedHashMap usersInfo) {
	StringBuilder useersData = new StringBuilder(&quot;&quot;);
	for (Object key : usersInfo.keySet()) {
		String keyString = (String)key;
		Object keyValue = usersInfo.get(keyString);
		useersData.append(keyValue + &quot;,&quot;);
	}
