            <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="Validate owner and customer ids" enabled="true">
              <stringProp name="BeanShellAssertion.query">import org.apache.commons.lang3.StringUtils;

String expectedPattern = vars.get(&quot;expected_pattern_for_id&quot;);
String divider = &quot;,&quot;;
String agentName = vars.get(&quot;agent_name&quot;);
String realOwnerId = vars.get(&quot;agent_id&quot;);
String ownersPrefix = &quot;ext_owner_id_&quot;;
String customersPrefix = &quot;ext_customer_id_&quot;;
String actualCustomersData = vars.get(&quot;ext_customer_id_ALL&quot;);
String actualOwnersData = vars.get(&quot;ext_owner_id_ALL&quot;);

private makeTestFailure(String failureReason) {
	Failure = true;
	FailureMessage = failureReason;
}

private makeTestFailure(String expectedValue, String expectedPattern, String actual) {
	Failure = true;
	FailureMessage = new StringBuilder(&quot;Response body error!\n&quot;)
		.append(&quot;Expected result for &quot; + expectedValue + &quot; user: \&quot;&quot; + expectedPattern + &quot;\&quot;\n&quot;)
		.append(&quot;Actual result: \&quot;&quot; + actual + &quot;\&quot;&quot;)
		.toString();
}

//---* Validate that all items are satisfy to given regex *---
public void validateByRegExp(String actualData, String prefix) {
	if (!actualData.isEmpty()) {
		int matches = StringUtils.countMatches(new String(actualData), divider);
		vars.put(&quot;items_count&quot;, String.valueOf(matches));
		int matchNum = Integer.parseInt(vars.get(&quot;items_count&quot;).trim()) + 1;

		for (int i = 1; i &lt;= matchNum; ++i) {
			String actualDataNext = prefix + i;
			actualDataNext =  vars.get(actualDataNext);
			if (!actualDataNext.matches(expectedPattern)) {
				makeTestFailure(realOwnerId, expectedPattern, actualDataNext);
			}
		}
	}
	else {
		makeTestFailure(&quot;Failed to retrieve customers ids.&quot;);
	}	
}

//---* Validate that all items are the same *---
public void validateAlloneness(String actualData, String prefix) {
	if (!actualData.isEmpty()) {
		int matches = StringUtils.countMatches(new String(actualData), divider);
		vars.put(&quot;items_count&quot;, String.valueOf(matches));
		int matchNum = Integer.parseInt(vars.get(&quot;items_count&quot;).trim()) + 1;

		for (int i = 1; i &lt;= matchNum; ++i) {
			String actualDataNext = prefix + i;
			actualDataNext =  vars.get(actualDataNext);
			if (!actualDataNext.equals(realOwnerId)) {
				makeTestFailure(agentName, realOwnerId, actualDataNext);
			}
		}
	}
	else {
		makeTestFailure(&quot;Failed to retrieve owners(agents) ids.&quot;);
	}
}

String actualCode = prev.getResponseCode();
if (actualCode != null) {
	vars.put(&quot;response_code&quot;, actualCode);
}

if (vars.get(&quot;response_code&quot;).equals(&quot;403&quot;)) {
	print(&quot;===============================================================&quot;);
	print(&quot;============ Thread ${__threadNum} : 403 forbidden ============&quot;);
	print(&quot;===============================================================&quot;);
	Failure = false;
	SampleResult.setSuccessful(true);
} else {
	validateAlloneness(actualOwnersData, ownersPrefix);
	validateByRegExp(actualCustomersData, customersPrefix);
}

//---* Log request size and count *---
String loopNumber = vars.get(&quot;loop_number&quot;);
if (Integer.parseInt(loopNumber) == 1) {
	String size = vars.get(&quot;page_size&quot;);
	String count = vars.get(&quot;data_count&quot;);
	String sizeDetails = &quot;Every request expects the content in this range: &lt;=&quot; + size;
	String countDetails = &quot;The count of the real data in the DB is: &quot; + count;
	if (count.isEmpty() || size.isEmpty()) {
		sizeDetails = &quot;Failed to retrieve the response page size&quot;;
		countDetails = &quot;Failed to retrieve the real data count&quot;;
	}

	String details = new StringBuilder(&quot;------------------------------------------------------\n&quot;)
		.append(&quot;     * Thread No.${__threadNum}: Request details     *\n&quot;)
		.append(sizeDetails + &quot;.\n&quot;)
		.append(countDetails + &quot;.\n&quot;)
		.append(&quot;------------------------------------------------------\n&quot;)
		.toString();
	print(details);
}</stringProp>
