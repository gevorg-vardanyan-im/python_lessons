              <stringProp name="BeanShellAssertion.query">import org.apache.commons.lang3.StringUtils;

//---* Response body assertion *---

//---* Validate that all items are satisfy to given regex *---
public void validateEmptiness(Map checkedValues) {

	for (String key : checkedValues.keySet()) {
		String actualData = checkedValues.get(key);

		if (!actualData.isEmpty()) {
			int matches = StringUtils.countMatches(new String(actualData), divider);
			vars.put(&quot;items_count&quot;, String.valueOf(matches));
			int matchNum = Integer.parseInt(vars.get(&quot;items_count&quot;).trim()) + 1;

			for (int i = 1; i &lt;= matchNum; ++i) {
				String actualDataNext = key + i;
				actualDataNext =  vars.get(actualDataNext);
				if (null == actualDataNext || actualDataNext.isEmpty()) {
			   		Failure = true;
			   		String fail = new StringBuilder(&quot;Response body error!\n&quot;)
                   			.append(&quot;User data expected to not be empty or null.\n&quot;)
                   			.append(&quot;Actual result for &quot; + i + &quot;-th user:\n&quot;)
                   			.append(key + i + &quot;: \&quot;&quot; + actualDataNext + &quot;\&quot;&quot;)
                   			.toString();
			   		FailureMessage = fail;
				}
			}
		}
		else {
			Failure = true;
			FailureMessage = &quot;Fail to retrieve customers ids.&quot;;
		}
	}
}

String divider = &quot;,&quot;;
String idsPrefix = &quot;user_id_&quot;;
String idsData = vars.get(&quot;user_id_ALL&quot;);
String firstNamesPrefix = &quot;first_name_&quot;;
String firstNamesData = vars.get(&quot;first_name_ALL&quot;);
String lastNamesPrefix = &quot;last_name_&quot;;
String lastNamessData = vars.get(&quot;last_name_ALL&quot;);

Map checkedValues = new HashMap();
checkedValues.put(idsPrefix, idsData);
checkedValues.put(firstNamesPrefix, firstNamesData);
checkedValues.put(lastNamesPrefix, lastNamessData);

validateEmptiness(checkedValues);


//---* Response code assertion *---

String actualCode = SampleResult.getResponseCode();
String expectedCode = &quot;200&quot;;
if (!actualCode.equals(expectedCode))  {
	Failure = true;
	FailureMessage = &quot;Response code error!&quot; + System.getProperty(&quot;line.separator&quot;) + &quot;Expected result: &quot; + expectedCode + System.getProperty(&quot;line.separator&quot;) + &quot;Actual result: &quot; + actualCode;
}

</stringProp>
