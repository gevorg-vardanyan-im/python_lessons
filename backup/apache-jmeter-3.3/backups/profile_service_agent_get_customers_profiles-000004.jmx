              <stringProp name="BeanShellAssertion.query">public void verifyValue(value) {

	if (value.isEmpty() || value == null) {
		Failure = true;
		FailureMessage = value + &quot; is null or emty&quot;;
	}
	
}

String id_number;
String first_name_number;
String last_name_number;

String emloyee_id;
String emloyee_first_name;
String emloyee_last_name;

String actualCode = prev.getResponseCode();
if (actualCode != null) {
	vars.put(&quot;response_code&quot;, actualCode);
}

if (vars.get(&quot;response_code&quot;).equals(&quot;403&quot;)) {
	
	Failure = false;
	SampleResult.setSuccessful(true);
	
} else {
	if (vars.get(&quot;id_matchNr&quot;) == null || vars.get(&quot;id_matchNr&quot;).isEmpty()) {
		Failure = true;
		FailureMessage = &quot;there is not any data&quot;;
	}

	int count = Integer.parseInt(vars.get(&quot;id_matchNr&quot;));
	for (int i = 1; i &lt;= count; i++) {
		id_number = &quot;id_&quot; + i;
		first_name_number = &quot;first_name_&quot; + i;
		last_name_number = &quot;last_name_&quot; + i;
		print(id_number);
		print(first_name_number);
		print(last_name_number);

		emloyee_id = vars.get(id_number);
		emloyee_first_name = vars.get(first_name_number);
		emloyee_last_name = vars.get(last_name_number);
		print(emloyee_id);

		verifyValue(emloyee_id);
		verifyValue(emloyee_first_name);
		verifyValue(emloyee_last_name);

	}
}
