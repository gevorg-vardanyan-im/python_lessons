              <stringProp name="script">import java.util.List;
import java.util.ArrayList;

String agent_id = vars.get(&quot;agent_id&quot;);
String agent_name = vars.get(&quot;agent_name&quot;);
String password = vars.get(&quot;password&quot;);
String ENV = vars.get(&quot;ENVIRONMENT&quot;);

String creator_id_number;
String message_id_number;
String is_mass_message_number;
String recalled_status_number;

String creator_id;
String message_id;
String is_mass_message;
String recalled_status;

int count = Integer.parseInt(vars.get(&quot;messageId_matchNr&quot;));

//log.info(&quot;count   &quot;+count);

ArrayList myList = new ArrayList();

for (int i = 1; i &lt;= count; i++) {

	creator_id_number = &quot;creatorId_&quot; + i;
	message_id_number = &quot;messageId_&quot; + i;
	is_mass_message_number = &quot;isMassMessage_&quot; + i;
	recalled_status_number = &quot;recalled_&quot; + i;

	creator_id = vars.get(creator_id_number);
	message_id = vars.get(message_id_number);
	is_mass_message = vars.get(is_mass_message_number);
	recalled_status = vars.get(recalled_status_number);

	//log.info(&quot;recalled_status is &quot; + recalled_status.equals(&quot;false&quot;));
	//log.info(&quot;creator_id &quot; + creator_id + &quot;agent_id &quot; + vars.get(&quot;agent_id&quot;));

	if (message_id != null &amp;&amp; !message_id.isEmpty() &amp;&amp; recalled_status.equals(&quot;false&quot;) &amp;&amp; !is_mass_message.equals(&quot;true&quot;) &amp;&amp; creator_id.equals(vars.get(&quot;agent_id&quot;))) {

		//log.info(&quot;creator_id &quot; + creator_id + &quot;agent_id &quot; + vars.get(&quot;agent_id&quot;));
		
		myList.add(message_id);
		
		//log.info(&quot;iiiiiiiiiiiii     &quot;+ i);
		
	}

}

if (myList.size() == 0) {

	ctx.getEngine().askThreadsToStop();
	
}

//log.info(myList.get(0));
//log.info(myList.get(0));
//log.info(myList.size());

bsh.shared.myArray${__threadNum} = myList;

