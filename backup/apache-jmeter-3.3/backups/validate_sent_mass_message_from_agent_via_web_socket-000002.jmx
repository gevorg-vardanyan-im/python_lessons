              <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="Increase an iterator" enabled="true">
                <boolProp name="resetInterpreter">false</boolProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="script">import org.apache.jmeter.util.JMeterUtils;
import org.apache.jmeter.threads.JMeterContextService;

String connection = &quot;_THREAD_IS_CONNECTED&quot;;
String property = &quot;${__threadNum}&quot; + connection;
int threads = JMeterContextService.getTotalThreads();
String all_threads = Integer.toString(threads);
String active_threads = Integer.toString(JMeterContextService.getNumberOfThreads());

JMeterUtils.setProperty(&quot;all_threads&quot;, all_threads);
JMeterUtils.setProperty(&quot;are_threads_connected&quot;, &quot;disconnected&quot;);
JMeterUtils.setProperty(&quot;is_message_sent&quot;, &quot;not_sent&quot;);
JMeterUtils.setProperty(&quot;is_first_loop&quot;, &quot;true&quot;);
JMeterUtils.setProperty(&quot;are_threads_received&quot;, &quot;not_received&quot;);

JMeterUtils.setProperty(property, &quot;true&quot;);
int check_all_threads = 0;

for (i = 2; i &lt;= threads; ++i) {
	String prop = Integer.toString(i) + connection;
	if(null != JMeterUtils.getProperty(prop)) {
		++check_all_threads;
	}
}

if (check_all_threads == (threads - 1)) {
	print(&quot;===== All threads are connected =====&quot;);
	JMeterUtils.setProperty(&quot;are_threads_connected&quot;, &quot;connected&quot;);
}</stringProp>
